{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "build": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}